{
    "name": "arkaitzgarro/erpdataio",
    "description": "ERPDataIO is a brigde to import/export data from ERP systems into Elcodi projects",
    "type": "library",
    "keywords": ["erp", "import", "export", "elcodi"],
    "license": "MIT",
    "authors": [
        {
            "name": "Arkaitz Garro",
            "email": "arkaitz.garro@gmail.com"
        },
        {
            "name": "Symfony Community",
            "homepage": "https://github.com/PaymentSuite/paymentsuite/contributors"
        }
    ],
    "require": {
        "php": ">=5.4",
        "symfony/config": ">=2.4.0",
        "symfony/http-kernel": ">=2.4.0",
        "symfony/dependency-injection": ">=2.4.0",
        "symfony/console": ">=2.4.0",
        "symfony/finder": "2.6.*@dev",
        "elcodi/product-bundle": "~0.1"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0",
        "raulfraile/ladybug-bundle": "dev-master",
        "matthiasnoback/symfony-dependency-injection-test": "0.*",
        "symfony/monolog-bridge": "~2.3",
        "doctrine/orm": "2.4.*",
        "doctrine/doctrine-bundle": "~1.2"
    },
    "replace": {
	    "arkaitzgarro/erpdataio-core-bundle": "self.version"
    },
    "suggest": {

    },
    "autoload": {
        "psr-4": {
            "ERPDataIO\\": "src/ERPDataIO"
        }
    },
    "minimum-stability": "stable",
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
